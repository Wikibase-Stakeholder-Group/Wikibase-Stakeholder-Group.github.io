source "https://rubygems.org"

# To updater or install dependencies, run:
#
#     bundle update
#
# To start a local server, run:
#
#     bundle exec jekyll serve
#
# NOTE: This file is ONLY used for local development.
# For security reasons, GitHub pages ignores this file and
# does not allow different versions or different packages.
# Instead, it alwys uses the latest version of github-pages.

gem "github-pages", group: :jekyll_plugins

# Workaround local fatal when using Ruby 3 or later.
# Jekyyll fixed this upstream in Jekyll 4, but GitHub remains on Jekyll 3.x.
# https://github.com/github/pages-gem/issues/752
# https://github.com/jekyll/jekyll/issues/8523
gem "webrick", "~> 1.8"

